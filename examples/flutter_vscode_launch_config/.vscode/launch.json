    "version": "0.2.0",
    "configurations": [
      {
        "name": "Launch Main Debug Mode",
        "request": "launch",
        "type": "dart",
        "flutterMode": "debug",
        "program": "lib/main.dart",
      },
      // 
      {
        "name": "Launch Main Profile Mode",
        "request": "launch",
        "type": "dart",
        "flutterMode": "profile",
        "program": "lib/main.dart",
      },
      // 
      // 
      {
        "name": "Launch Main Release Mode",
        "request": "launch",
        "type": "dart",
        "flutterMode": "release",
        "program": "lib/main.dart",
      },
      // 
      // 
      {
        "name": "Launch Main Development",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "--flavor",
          "development",
          "--target", 
          "lib/main_development.dart",
        ]
      },
      {
        "name": "Launch Main Staging",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "--flavor",
          "staging",
          "--target", 
          "lib/main_staging.dart",
        ]
      },
      {
        "name": "Launch Main Production",
        "request": "launch",
        "type": "dart",
        "program": "lib/main.dart",
        "args": [
          "--flavor",
          "production",
          "--target", 
          "lib/main_production.dart",
        ]
      }
      // 
    ]
  }
  